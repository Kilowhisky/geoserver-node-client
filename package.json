{
  "name": "geoserver-node-client",
  "version": "1.2.3",
  "description": "Node.js client for GeoServer REST API",
  "type": "module",
  "main": "geoserver-rest-client.ts",
  "scripts": {
    "demo": "ts-node demo/index.ts",
    "docs": "typedoc geoserver-rest-client.ts src/*.ts DOCS_HOME.md --out out && cp img/*.png out/",
    "lint": "eslint src/**/*.ts test/**/*.ts scripts/**/*.ts geoserver-rest-client.ts",
    "lint-fix": "eslint --fix  src/**/*.ts test/**/*.ts scripts/**/*.ts geoserver-rest-client.ts",
    "pretest": "npm run lint",
    "test": "ts-mocha -p ./tsconfig.json ./test/test.ts --timeout 10000",
    "release": "release-it",
    "build": "npm run build:clean && npm run build:ts",
    "build:clean": "rm -rf dist",
    "build:ts": "tsc"
  },
  "exports": {
    "import": "./geoserver-rest-client.js",
    "require": "./dist/geoserver-rest-client.js"
  },
  "files": [
    "geoserver-rest-client.js",
    "src",
    "dist"
  ],
  "keywords": [
    "GeoServer",
    "REST",
    "Client"
  ],
  "author": "C. Mayer, meggsimum (info_at*meggsimum?dot?de)",
  "license": "BSD-2-Clause",
  "dependencies": {
    "node-fetch": "^2.6.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "mocha": "^9.1.3",
    "release-it": "^15.4.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4"
  }
}
